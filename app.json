{
  "expo": {
    "name": "MediaHub",
    "slug": "MediaHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/mh_logo_tb.png",
      "resizeMode": "contain",
      "backgroundColor": "#303030"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/mh_logo_bg_g.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mediahub"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
